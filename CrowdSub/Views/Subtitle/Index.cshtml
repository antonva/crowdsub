@model IEnumerable<CrowdSub.Models.subtitle>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.subtitle_file_path)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.subtitle_version_is_active)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.subtitle_version)
        </th>
        <th>

        <td>
            @Html.DisplayFor(modelItem => item.subtitle_file_path)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.subtitle_version_is_active)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.subtitle_version)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.user.user_name)
        </td>
